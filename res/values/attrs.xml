<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomDigitalClock">
        <attr name="format12Hr" format="string"/>
        <attr name="format24Hr" format="string"/>
        <attr name="capitalize" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="AutoAdjustTextView">
        <attr name="fontPath" format="string"/>
        <attr name="minFontSizeSp" format="integer"/>
        <attr name="maxFontSizeSp" format="integer"/>
    </declare-styleable>
    <declare-styleable name="DateFormatPreference">
        <attr name="formatType" format="string"/>
    </declare-styleable>
    <declare-styleable name="SideMenu">
        <attr name="SideMenuIconColor" format="color"/>
        <attr name="SideMenuBackground" format="reference"/>
        <attr name="SideMenuIconBackground" format="reference"/>
    </declare-styleable>
</resources>
